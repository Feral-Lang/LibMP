/* Mandelbrot Set
*/

let io = import('std/io');
let mp = import('mp/mp');

for let y = -1.0; y < 1.01; y += 0.05 {
    for let x = -2.05; x <= 0.55; x += 0.03 {
        let xy = mp.newComplex(x, y);
        let z = mp.newComplex();
        for i in irange(0, 20) {
            z = (z ** 2) + xy;
        }
        if z.abs() < 2.0 { io.print(' '); }
        else { io.print(';'); }
    }
    io.println();
}